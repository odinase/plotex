classdef Subplotex < Plotex
    %SUBPLOTEX Summary of this class goes here
    %   Detailed explanation goes here
    
    properties (SetAccess = private)
        plots;
    end
    
    methods
        function obj = Subplotex(plots,varargin)
            p = inputParser;            
            
            isPlotex = @(x) isa(x, 'Plotex');
            valid_plots = @(x) all(cellfun(isPlotex, x));
            valid_label = @(x) ischar(x) || isstring(x);
            
            addRequired(p, 'plots', valid_plots);
            addOptional(p, 'title', valid_label);
            
            parse(p, plots, varargin);
                        
            obj.values = p.Results.data.values;
            obj.time = p.Results.data.time;
            obj.legend = p.Results.legend;
        end
        
        function outputArg = method1(obj,inputArg)
            %METHOD1 Summary of this method goes here
            %   Detailed explanation goes here
            outputArg = obj.Property1 + inputArg;
        end
    end
end

